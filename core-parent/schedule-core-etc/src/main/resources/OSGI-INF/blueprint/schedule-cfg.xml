<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
    <bean id="scheduleApplicationConfig" class="com.kalix.framework.core.impl.web.BaseApplicationConfig">
        <property name="bundleContext" ref="blueprintBundleContext"/>
    </bean>
    <service id="managedService" interface="org.osgi.service.cm.ManagedService" ref="scheduleApplicationConfig">
        <service-properties>
            <entry key="service.pid" value="config.schedule.app"/>
        </service-properties>
    </service>

    <bean id="scheduleDictConfig" class="com.kalix.framework.core.impl.system.BaseDictConfig">
    </bean>
    <service  interface="org.osgi.service.cm.ManagedService" ref="scheduleDictConfig">
        <service-properties>
            <entry key="service.pid" value="config.schedule.dict"/>
        </service-properties>
    </service>
</blueprint>